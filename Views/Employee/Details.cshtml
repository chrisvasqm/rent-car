@model RentCar.Views.Model.Employee

@{
    ViewBag.Title = "Details";
    Layout = "_Layout";
}

<h2>Details</h2>

<ul>
    <li>Name: @Model.Name</li>
    <li>Identification Card: @Model.IdentificationCard</li>
    <li>Status: @Model.Status.Name</li>
    <li>Commission: @Model.Commission.Value</li>
    <li>Work shift: @Model.WorkShift.Name</li>
    <li>Admission date: @Model.AdmissionDate</li>
</ul>