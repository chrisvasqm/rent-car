@model RentCar.Views.Model.Client

@{
    ViewBag.Title = "Details";
    Layout = "_Layout";
}

<h2>Details</h2>

<ul>
    <li>Name: @Model.Name</li>
    <li>Identification Card: @Model.IdentificationCard</li>
    <li>Credit Card: @Model.CreditCard</li>
    <li>Credit Limit: @Model.CreditLimit</li>
    <li>Person Type: @Model.PersonType.Name</li>
    <li>Status: @Model.Status.Name</li>
</ul>